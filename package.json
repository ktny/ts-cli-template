{
  "name": "ts-cli-template",
  "version": "0.1.0",
  "description": "A modern TypeScript CLI tool template with React Ink",
  "type": "module",
  "main": "dist/simple.js",
  "bin": {
    "ts-cli-template": "dist/simple.js"
  },
  "exports": {
    ".": {
      "import": "./dist/simple.js",
      "types": "./dist/simple.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsx --tsconfig tsxconfig.json src/simple.tsx",
    "build": "tsc",
    "start": "node dist/simple.js"
  },
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ktny/ts-cli-template.git"
  },
  "bugs": {
    "url": "https://github.com/ktny/ts-cli-template/issues"
  },
  "homepage": "https://github.com/ktny/ts-cli-template#readme",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "ink": "^5.0.1",
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^24.0.10",
    "@types/react": "^18.3.0",
    "eslint": "^9.15.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
