  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "ink": "^5.0.1",
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^24.0.10",
    "@types/react": "^18.3.0",
    "tsx": "^4.20.3",
